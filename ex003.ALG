algoritmo "semnome"
var
   funcionario: caractere
   salario: real
inicio
      Escreval ("Qual o nome do funcionario(a)? ")
      leia (funcionario)
      Escreva ("Qual é o seu salario? ")
      leia (salario)
      Escreva ("O funcionario ", funcionario, " tem um salario de R$ ", salario)
      
fimalgoritmo